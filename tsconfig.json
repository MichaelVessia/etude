{
  "compilerOptions": {
    "target": "ES2022",
    "moduleDetection": "force",
    "verbatimModuleSyntax": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noUnusedLocals": true,
    "noImplicitOverride": true,
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ]
  }
}
